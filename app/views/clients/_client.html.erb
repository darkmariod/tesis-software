<div id="<%= dom_id client %>">
  <p>
    <strong>Nombre cliente:</strong>
    <%= client.nombre_cliente %>
  </p>

  <p>
    <strong>Codigo cliente:</strong>
    <%= client.codigo_cliente %>
  </p>

  <p>
    <strong>Ruc:</strong>
    <%= client.ruc %>
  </p>

  <p>
    <strong>Direccion:</strong>
    <%= client.direccion %>
  </p>

  <p>
    <strong>Telefono:</strong>
    <%= client.telefono %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= client.email %>
  </p>

  <p>
    <strong>Fecha falla del equipo:</strong>
    <%= client.fecha_falla_del_equipo %>
  </p>

  <p>
    <strong>Tecnico a cargo:</strong>
    <%= client.tecnico_a_cargo %>
  </p>

  <p>
    <strong>Hora de inicio:</strong>
    <%= client.hora_de_inicio %>
  </p>

  <p>
    <strong>Hora final:</strong>
    <%= client.hora_final %>
  </p>

  <p>
    <strong>Trabajo realizado:</strong>
    <%= client.trabajo_realizado %>
  </p>

  <p>
    <strong>Observaciones del tecnico:</strong>
    <%= client.observaciones_del_tecnico %>
  </p>

  <p>
    <strong>Firma del cliente:</strong>
    <%= client.firma_del_cliente %>
  </p>

</div>
